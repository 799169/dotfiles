[push]
	default = simple
[alias]
	st = status -sb
	fe = fetch
	co = checkout
	br = branch
	cm = commit
	ca = commit -a
	caa = commit -a --amend
	rb = rebase
	git = !git
[core]
	excludesfile = /home/andrew/.gitignore_global
[merge]
	conflictstyle = diff3
[pull]
	rebase = true
[rebase]
	autosquash = true

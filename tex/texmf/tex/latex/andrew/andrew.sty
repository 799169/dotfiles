% Subproof with shaded square, subsubproof with //
\newenvironment{subproof}[1][\proofname]{%
  \renewcommand{\qedsymbol}{$\blacksquare$}%
  \begin{proof}[#1]%
}{%
  \end{proof}%
}
\newenvironment{subsubproof}[1][\proofname]{%
  \renewcommand{\qedsymbol}{//}%
  \begin{proof}[#1]%
}{%
  \end{proof}%
}


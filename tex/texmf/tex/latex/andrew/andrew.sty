\newcommand{\handout}[5]{
  \noindent
  \begin{center}
  \framebox{
    \vbox{
      \hbox to 6.00in { {\bf #1} \hfill #2 }
      \vspace{4mm}
      \hbox to 6.00in { {\Large \hfill #5  \hfill} }
      \vspace{2mm}
      \hbox to 6.00in { {\em #3 \hfill #4} }
    }
  }
  \end{center}
  \vspace*{4mm}
}

\newcommand{\titlebox}{
	\handout{\courseName}{\term}{\professor}{\textbf{\authorName}\ifdef{\collaborators}{
			with \ifdefempty{\collaborators}{no collaborators}{\collaborators}}{}}{\handoutName}
	\thispagestyle{empty}
}

